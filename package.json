{
  "name": "sequelize-query-builder",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test node --max-old-space-size=8192 --expose-gc ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit --logHeapUsage --verbose --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit --logHeapUsage --verbose --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "preseed": "rimraf dist && nest build",
    "seed": "node ./dist/_common/database/seeders",
    "migrate": "npx sequelize-cli db:migrate",
    "migration:new": "cross-env-shell sequelize migration:generate --name $MIG_NAME"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "sequelize": "^6.37.5",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5"
  }
}
